@using MSITTeam1Admin.ViewModels
@model backGrade
@{
    ViewData["Title"] = "skillIndex";
}

<div class="container">
    <div class="col-12">
        <div class="page-title my-4">
            <h2>技能成績檢視</h2>
        </div>
    </div>
    <section id="main-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="bootstrap-data-table-panel">
                        <div class="table-responsive">
                            <table id="bootstrap-data-table-export" class="table table-striped table-bordered my-3">
                                <thead>
                                    <tr>
                                        <th>帳號</th>
                                        <th>姓名</th>
                                        <th>技能</th>
                                        <th>成績</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        foreach (var item in Model.TSkillGrade)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.FAccount)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.FMemberName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.FSkillCategory)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.FGrade)
                                                </td>
                                            </tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>


