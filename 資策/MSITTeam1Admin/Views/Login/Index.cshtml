<div class="unix-login">
    <div class="container-fluid">
        <div class="row justify-content-center">
            <div class="col-md-6 col-xl-4">
                <div class="login-content">
                    <div class="login-logo">
                        <span><img src="~/images/logo.png" alt="" /></span>
                    </div>
                    <div class="login-form shadow">
                        <h3 class="text-center pb-2">資展國際後台管理系統</h3>
                        <form>
                            <div class="form-group">
                                <label>帳號</label>
                                <input type="text" id="LAccount" name="LAccount" class="form-control" placeholder="請輸入帳號">
                            </div>
                            <div class="form-group">
                                <label>密碼</label>
                                <input type="password" id="LPassword" name="LPassword" class="form-control" placeholder="請輸入密碼">
                            </div>
                            <button type="submit" asp-action="Login" class="btn btn-primary btn-flat m-b-30 m-t-30">登入</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



